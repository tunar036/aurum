{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "astrotomic/laravel-translatable": "^11.9",
        "bensampo/laravel-enum": "4.*",
        "brotzka/laravel-dotenv-editor": "^2.1",
        "bumbummen99/shoppingcart": "^4.1",
        "cyrildewit/eloquent-viewable": "^6.1",
        "dyrynda/laravel-cascade-soft-deletes": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "joedixon/laravel-translation": "^1.1",
        "laravel/framework": "^8.54",
        "laravel/jetstream": "^2.6",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.10",
        "orangehill/iseed": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^2.1",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^5.2",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "symfony/postmark-mailer": "^6.0",
        "unisharp/laravel-filemanager": "^2.4",
        "webpatser/laravel-uuid": "^4.0",
        "webreinvent/laravel-nestable": "^0.0.3",
        "yajra/laravel-datatables-buttons": "^4.10",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-query-detector": "^1.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
